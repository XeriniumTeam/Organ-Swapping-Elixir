            <h4>Genes:</h4>
            {{#each genesDonor}}
            <label for="donorGenes_{{@key}}">{{@key}}:</label>
            <input type="text" id="donorGenes_{{@key}}_1" name="donor[genes][{{@key}}][]" placeholder="Allele 1" required>
            <input type="text" id="donorGenes_{{@key}}_2" name="donor[genes][{{@key}}][]" placeholder="Allele 2" required>
            {{/each}}
            <button type="submit">Submit</button>
        </form>
    </div>
    <script src="http://localhost:5000/js/hla.js"></script>
</body>
</html>
